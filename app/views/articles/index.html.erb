<h1>Articles</h1>

<%= link_to 'Novo article', new_article_path %>

<ul>
  <% @articles.each do |article| %>
      <li>
       <%= link_to article.title, article %>
      </li>
  <% end %>
</ul>

<button type="button" class="btn btn-primary" data-bs-toggle="tooltip" data-bs-placement="top" title="Tooltip on top">
  Tooltip on top
</button>
